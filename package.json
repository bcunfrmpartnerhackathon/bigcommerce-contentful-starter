{
  "name": "@uniformdev/bigcommerce-contentful-starter",
  "version": "9.0.2",
  "private": true,
  "scripts": {
    "uniform": "uniform",
    "dev:cfw": "run-s build export dev:worker",
    "dev:next": "next dev -p 3001",
    "build": "run-s generate build:next",
    "build:next": "next build",
    "export": "next export",
    "ci:build": "npm run generate && next build",
    "generate": "uniform optimize manifest download --output ./src/lib/intentManifest.json --apiKey $UNIFORM_API_KEY",
    "start": "next start",
    "dev": "run-s generate dev:next",
    "build:worker": "wrangler build",
    "dev:worker": "wrangler dev",
    "pull": "run-p pull:components pull:compositions",
    "pull:components": "uniform presentation component pull ./data/components",
    "pull:compositions": "uniform presentation composition pull ./data/compositions",
    "push": "run-p push:components push:compositions",
    "push:components": "uniform presentation component push ./data/components",
    "push:compositions": "uniform presentation composition push ./data/compositions"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^15.1.0",
    "@reach/rect": "^0.15.0",
    "@uniformdev/optimize-tracker-common": "^10.0.0",
    "@uniformdev/optimize-tracker-react": "^10.0.0",
    "@uniformdev/upm": "^9.0.2-alpha.33",
    "@uniformdev/upm-bigcommerce": "^10.0.0",
    "@uniformdev/upm-contentful": "^10.0.0",
    "@uniformdev/upm-react": "^10.0.0",
    "base64-string": "^1.1.2",
    "classnames": "^2.2.6",
    "contentful": "^8.4.2",
    "contrast-color": "^1.0.1",
    "cross-env": "^7.0.3",
    "focus-trap-react": "^8.3.2",
    "framer-motion": "^4.0.3",
    "js-cookie": "^2.2.1",
    "jsondiffpatch": "^0.4.1",
    "keen-slider": "^5.4.0",
    "marqy": "^0.0.8",
    "next": "^11.1.0",
    "next-themes": "^0.0.15",
    "nookies": "^2.5.2",
    "qs": "^6.10.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.10.1",
    "react-keyed-flatten-children": "^1.3.0",
    "swr": "^0.5.4",
    "tailwindcss": "^2.2.7",
    "use-intersection": "^0.2.1"
