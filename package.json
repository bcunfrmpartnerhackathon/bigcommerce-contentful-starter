  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/cookie": "^0.4.0",
    "@types/node": "^16.0.1",
    "@types/react": "^17.0.15",
    "@uniformdev/cli": "^10.0.0",
    "autoprefixer": "^10.3.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.5",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.3.5"
  }
}
